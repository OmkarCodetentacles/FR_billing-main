        /*padding: 10px;*/
        background-color: #ffffff;
        cursor: pointer;
    }

    .bannerDiv {
        width: 100%;
    }

    .text-truncate {
        white-space: nowrap;
        overflow: hidden;
        text-overflow: ellipsis;
    }

    .notify-badge {
        position: absolute;
        /* right: 632px; */
        top: 96px;
        background: #da2f2f;
        text-align: center;
        border-radius: 30px 30px 30px 30px;
        color: white;
        padding: 2px 8px;
        font-size: 17px;
    }

    .modal-body img {
        cursor: pointer;
    }

    .share-buttons {
        display: flex;
        justify-content: space-around;
        margin-top: 20px;
    }

        .share-buttons a {
            text-decoration: none;
        }

    .modal-content {
        border-radius: 50px;
        background-color: #f9f9f9;
        border: 1px solid #ccc;
    }

    .modal-header {
        border-bottom: none;
    }

    .modal-footer {
        border-top: none;
    }

    .modal-title {
        color: #333;
    }

    .close {
        color: #999;
    }

    .share-buttons a {
        margin-right: 10px;
    }

    .share-buttons img {
        transition: transform 0.3s ease;
    }

        .share-buttons img:hover {
            transform: scale(1.1);
        }

    .btn {
        height: 36px !important;
    }

    .btn-danger {
        /* background-color: #dd4b39; */
        /* border-color: #d73925; */
        /* border-radius: 0; */
    }


    .glow {
        font-family: cursive;
        font-size: 15px;
        color: #990000;
        text-align: center;
        animation: glow 1s ease-in-out infinite alternate;
    }


    /* Status Dot Animation */
    .status-dot {
        width: 10px;
        height: 10px;
        background-color: #198754;
        border-radius: 50%;
        display: inline-block;
        /*margin-right: 8px;*/
        position: relative;
    }

        .status-dot::after {
            content: '';
            position: absolute;
            top: -2px;
            left: -2px;
            width: 14px;
            height: 14px;
            background-color: #198754;
            border-radius: 50%;
            opacity: 0.5;
            animation: pulse 2s ease-in-out infinite;
        }

    @@keyframes pulse {
        0% {
            transform: scale(1);
            opacity: 0.5;
        }

        50% {
            transform: scale(1.5);
            opacity: 0;
        }

        100% {
            transform: scale(1);
            opacity: 0.5;
        }
    }

    @@-webkit-keyframes glow {
        from {
            text-shadow: 0 0 10px #fff, 0 0 20px #fff, 0 0 30px #e60073, 0 0 40px #e60073, 0 0 50px #e60073, 0 0 60px #e60073, 0 0 70px #e60073;
        }

        to {
            text-shadow: 0 0 20px #fff, 0 0 30px #ff4da6, 0 0 40px #ff4da6, 0 0 50px #ff4da6, 0 0 60px #ff4da6, 0 0 70px #ff4da6, 0 0 80px #ff4da6;
        }
    }
</style>



