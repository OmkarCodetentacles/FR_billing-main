<script>
    $(document).ready(function () {
           document.addEventListener('DOMContentLoaded', (event) => {
               var yeardata = '@yeardata';
               if (yeardata != "" || yeardata!=null) {
            var dropdown = document.getElementById('yearStartDropdown');
            dropdown.value = yeardata;
        }
    });
        $('#yearStartDropdown').change(function () {
            var selectedYears = $(this).val();
            var confirmChange = confirm("Are you sure you want to change the  year(s) ?");
            if (confirmChange) {
                $.ajax({
                    url: '/Admin/UpdateYearStart',
                    type: 'GET',
                    data: { year: selectedYears },
                    success: function (response) {
                        if (response.success) {
                            alert("Start year has been updated to " + response.yearStart);
                        } else {
                            alert("Failed to update start year");
                        }
                    },
                    error: function () {
                        alert("Something went wrong!");
                    }
                });
            }
        });
    });
</script>
