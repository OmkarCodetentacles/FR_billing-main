                        <div class="form-group">
                            @using (Html.BeginForm("importFromExcel", "Booking", FormMethod.Post, new { enctype = "multipart/form-data" }))
                            {
                                <div class="col-md-12 no-padding">
                                    <label for="file"><strong id="up-txt"> Upload for Document </strong></label>
                                    <input type="file" name="httpPostedFileBase" id="file" class="form-control" />
                                    <div style="display:flex; gap:10px;justify-content:end;margin-top:10px">
                                        <input type="submit" value="Upload" class="btn btn-primary" />
                                        <a href="http://frbilling.com/excelformat/UploadforDocument.xlsx" class="btn btn-success"> Export Excel</a>
                                    </div>
                                </div>


                            }
                        </div>
