
@{
    ViewBag.Title = "PrintBulkCashReceipt";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<section class=" content-wrapper">

    <div class="container">
        <div class="row">
            <div class="col-xs-12">
                <div class="box box-info" style="padding: 16px 16px !important">
                    <div class="box-header with-border">
                        <h4>Print Bulk Cash Receipt</h4>
                    </div>
                    <div class="box-body">
                        <div class="row">
                            <div class="form-horizontal">

                                @using (Html.BeginForm())
                                {
                                    <span class="text-danger">@Html.ValidationMessage("required")</span>
                                    <span class="text-danger">@Html.ValidationMessage("seriesNotmatch")</span>
                                    <div class="col-sm-10 form-group mb-15">
                                        <label for="period-from" class="col-sm-6 control-label">From Consignment No </label>
                                        <div class="col-sm-6 input-group date">


                                            <input type="text" class="form-control pull-right" name="FromConsignment" id="FromConsignment" required>

                                        </div>
                                    </div>
                                    <div class="col-sm-10 form-group mb-15">
                                        <label for="period-to" class="col-sm-6 control-label">To Consignment No</label>
                                        <div class="col-sm-6 input-group date">


                                            <input type="text" class="form-control pull-right" id="ToConsignment" name="ToConsignment" required>

                                        </div>
                                    </div>
                                    <div class="col-sm-10 form-group mb-15">
                                        <input type="submit" value="Download" class="btn btn-primary pull-right sbmt" />
                                    </div>



                                }
                            </div>
                        </div>

                        <br>
                        <!-- /input-group -->
                        <!-- /input-group -->

                    </div>
                </div>
                <!-- /.box-body -->
            </div>
            <!-- /.box -->
        </div>
    </div>
    <!-- /.col -->

</section>
<!-- /.content -->


