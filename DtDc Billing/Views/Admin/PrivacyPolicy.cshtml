        .new-blue-button-hover {
            position: relative;
            display: inline-block;
            padding: 10px 20px;
            background-color: #007bff;
            color: #fff;
            text-decoration: none;
            border-radius: 4px;
            overflow: hidden;
            transition: color 0.3s ease, font-size 0.3s ease;
            animation: shake 0.3s ease infinite;
        }

            .new-blue-button-hover a {
                color: inherit;
            }

            .new-blue-button-hover::after {
                content: '';
                position: absolute;
                background: rgba(255, 255, 255, 0.3);
                border-radius: 50%;
                width: 100px;
                height: 100px;
                top: 50%;
                left: 50%;
                transform: translate(-50%, -50%);
                transition: width 0.3s ease, height 0.3s ease;
                pointer-events: none;
            }

            .new-blue-button-hover:hover::after {
                width: 200px;
                height: 200px;
            }

            .new-blue-button-hover:hover {
                animation-play-state: paused; /* Pause the shake animation on hover */
            }

        .main-banner .item h1 {
            font-size: 50px;
            font-weight: 700;
            color: #2a2a2a;
            line-height: 72px;
        }

            .main-banner .item h1 em {
                color: #03a4ed;
                font-style: normal;
            }

        @@keyframes shake {
            0% {
                transform: translateX(0);
            }

            25% {
                transform: translateX(-5px);
            }

            50% {
                transform: translateX(5px);
            }

            75% {
                transform: translateX(-3px);
            }

            100% {
                transform: translateX(0);
            }
        }


        .diwalilogo img {
            width: 18%;
            overflow: hidden;
        }

        .diwalilogofooter img {
            width: 56%;
            overflow: hidden;
        }
    </style>


